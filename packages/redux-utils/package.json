      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@redux-observable-backend/core": "^4.1.0",
    "rxjs": "^6.5.2"
  },
  "peerDependencies": {
    "@redux-observable-backend/core": ">4 <5",
    "rxjs": ">=6.2.2 <7"
  }
