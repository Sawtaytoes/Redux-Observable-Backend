{
  "name": "@redux-observable-backend/node",
  "version": "4.3.2",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$redux": "./redux"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "redux/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "better-module-alias": "^1.0.1",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@redux-observable-backend/core": "^4.3.2",
    "@redux-observable-backend/redux-utils": "^4.3.2"
  },
  "peerDependencies": {
    "@redux-observable-backend/core": ">4 <5",
    "@redux-observable-backend/redux-utils": ">4 <5",
    "rxjs": "^6"
  }
}
