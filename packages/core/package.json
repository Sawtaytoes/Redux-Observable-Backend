{
  "name": "@redux-observable-backend/core",
  "version": "4.4.0",
  "description": "Core packages Redux-Observable Backend.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "eslint .",
    "start": "yarn && eslint . && nodemon app.js",
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:debug:watch": "ava --watch --verbose",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "better-module-alias": "^1.0.1",
    "chalk": "^2.4.2",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@ghadyani-eslint/eslint-config-node": "^1.1.6",
    "eslint": "^6.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sawtaytoes/Redux-Observable-Backend.git"
  }
}
