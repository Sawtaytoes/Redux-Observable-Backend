{
  "name": "@redux-observable-backend/core",
  "version": "4.1.0",
  "description": "Core packages Redux-Observable Backend.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect .",
    "lint": "eslint .",
    "start": "yarn && eslint . && nodemon .",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "better-module-alias": "^1.0.0",
    "chalk": "^2.4.2",
    "rxjs": "^6.5.2"
  }
}
