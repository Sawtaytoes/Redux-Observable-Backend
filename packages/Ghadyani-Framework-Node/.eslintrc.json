{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true,
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "no-console": "off",
    "no-unused-vars": "warn",
    "semi": [
      "error",
      "never"
    ],
    "strict": [
      "warn",
      "never"
    ]
  }
}
