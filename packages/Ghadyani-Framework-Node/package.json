{
  "name": "ghadyani-framework-node",
  "version": "0.0.0",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "lint": "node --experimental-modules . lint",
    "lint:watch": "nodemon . lint",
    "start": "yarn && yarn node --experimental-modules .",
    "test": "yarn lint && ava",
    "test:watch": "yarn lint && ava --watch"
  },
  "engines": {
    "node": "^10.7.0",
    "yarn": "^1.6.0"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "concurrency": 5,
    "files": [
      "scripts/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "ava": "^1.0.0-beta.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "eslint": "^5.0.0",
    "eslint-plugin-compat": "^2.4.0",
    "express": "^4.16.3",
    "express-sslify": "^1.2.0",
    "helmet": "^3.12.1",
    "is-equal": "^1.5.5",
    "nodemon": "^1.17.5",
    "rxjs": "^6.2.2",
    "uuid": "^3.2.1",
    "ws": "^5.2.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
