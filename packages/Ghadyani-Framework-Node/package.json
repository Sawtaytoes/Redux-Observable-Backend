{
  "name": "@ghadyani-framework/node",
  "version": "2.1.0",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": "^10.7.0",
    "yarn": "^1.7.0"
  },
  "_moduleAliases": {
    "$redux": "./redux"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "redux/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@ghadyani-framework/base": "^0.0.0",
    "is-equal": "^1.5.5",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-observable": "^1.0.0",
    "uuid": "^3.3.2",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@ghadyani-framework/redux-utils": "^0.0.1"
  },
  "peerDependencies": {
    "@ghadyani-framework/redux-utils": "^0.x.x"
  }
}
