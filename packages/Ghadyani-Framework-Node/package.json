{
  "name": "@ghadyani-framework/node",
  "version": "0.0.0",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node . lint",
    "lint:watch": "nodemon . lint",
    "start": "yarn && nodemon app.js",
    "test": "yarn lint && ava",
    "test:watch": "yarn lint && ava --watch"
  },
  "engines": {
    "node": "^10.7.0",
    "yarn": "^1.7.0"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "concurrency": 5,
    "files": [
      "scripts/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@ghadyani-framework/eslint-config-ghadyani-framework-base": "^0.0.0",
    "@ghadyani-framework/eslint-config-ghadyani-framework-node": "^0.0.1",
    "app-module-path": "^2.2.0",
    "ava": "^1.0.0-beta.6",
    "chalk": "^2.4.1",
    "is-equal": "^1.5.5",
    "nodemon": "^1.18.3",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2",
    "yargs": "^12.0.1"
  }
}
