{
  "name": "@ghadyani-framework/node",
  "version": "3.0.2",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$redux": "./redux"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "redux/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@ghadyani-framework/base": "^0.3.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-observable": "^1.1.0",
    "uuid": "^3.3.2",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@ghadyani-framework/redux-utils": "^1.1.3",
    "rxjs": "^6.5.2"
  },
  "peerDependencies": {
    "@ghadyani-framework/redux-utils": ">=1.1.2 <2",
    "rxjs": ">=6.2.2 <7"
  }
}
