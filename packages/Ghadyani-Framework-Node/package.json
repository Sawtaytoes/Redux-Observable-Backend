{
  "name": "@ghadyani-framework/node",
  "version": "1.1.0",
  "description": "Utilities for creating Node.js applications.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": "^10.7.0",
    "yarn": "^1.7.0"
  },
  "_moduleAliases": {
    "$redux": "./redux",
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "concurrency": 5,
    "files": [
      "redux/**/*.test.js",
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@ghadyani-framework/eslint-config-base": "^0.0.3",
    "@ghadyani-framework/eslint-config-node": "^0.0.2",
    "@ghadyani-framework/setup-module-aliases": "^0.0.0",
    "ava": "^1.0.0-beta.6",
    "chalk": "^2.4.1",
    "is-equal": "^1.5.5",
    "module-alias": "^2.1.0",
    "nodemon": "^1.18.3",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2",
    "yargs": "^12.0.1"
  }
}
