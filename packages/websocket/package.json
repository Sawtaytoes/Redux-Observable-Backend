{
  "name": "@redux-observable-backend/websocket",
  "version": "4.3.5",
  "description": "Utilities for creating applications using WebSockets.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:debug": "ava --verbose",
    "test:debug:watch": "ava --watch --verbose",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$redux": "./redux",
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "redux/**/*.test.js",
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@redux-observable-backend/node": "^4.3.5",
    "@redux-observable-backend/redux-utils": "^4.3.5",
    "better-module-alias": "^1.0.1",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@ghadyani-eslint/eslint-config-node": "^1.1.6",
    "eslint": "^6.5.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sawtaytoes/Redux-Observable-Backend.git"
  }
}
