{
  "name": "@redux-observable-backend/websocket",
  "version": "4.2.4",
  "description": "Utilities for creating applications using WebSockets.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "yarn && nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$redux": "./redux",
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "files": [
      "redux/**/*.test.js",
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "better-module-alias": "^1.0.0",
    "ws": "^7.0.1"
  },
  "devDependencies": {
    "@redux-observable-backend/node": "^4.2.4",
    "@redux-observable-backend/redux-utils": "^4.2.4",
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "peerDependencies": {
    "@redux-observable-backend/node": ">4 <5",
    "@redux-observable-backend/redux-utils": ">4 <5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
